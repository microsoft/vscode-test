// Place your settings in this file to overwrite default and user settings.
{
    "editor.insertSpaces": true,
	"files.eol": "\n",
	"files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
	},
	"prettier.semi": true
}