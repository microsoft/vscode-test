{
	"rules": {
		"indent": [
			true,
			"tabs"
		],
		"no-unused-expression": true,
		"no-unreachable": true,
		"no-duplicate-variable": true,
		"no-duplicate-key": true,
		"no-unused-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": true,
		"triple-equals": true,
		"no-unexternalized-strings": [
			true,
			{
				"signatures": [
					"localize",
					"nls.localize"
				],
				"keyIndex": 0,
				"messageIndex": 1
			}
		]
	}
}